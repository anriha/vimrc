{
    "python.jediEnabled": false,
    "python.linting.pep8Args": ["--max-line-length=119"],
    "python.linting.flake8Args": ["--max-line-length=119"],
    "python.linting.pylintArgs": ["--generate-members"],
    "python.autoComplete.extraPaths": ["/home/anriha/.local/lib/python3.8/site-packages/"],
    "python.linting.mypyEnabled": true,
    "languageserver": {
      "ccls": {
        "command": "ccls",
